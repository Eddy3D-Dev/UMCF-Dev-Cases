FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1.2;

boundaryField
{
    left
    {
        type            slip;
    }
    right
    {
        type            slip;
    }
    back
    {
        type            zeroGradient;
    }
    front
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.15;
        value           nonuniform List<scalar> 0();
    }
    bottom
    {
        type            kqRWallFunction;
        value           uniform 1.2;
    }
    top
    {
        type            slip;
    }
    Building
    {
        type            kqRWallFunction;
        value           uniform 1.2;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 1.2;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 1.2;
    }
    procBoundary2to5
    {
        type            processor;
        value           uniform 1.2;
    }
    procBoundary2to6
    {
        type            processor;
        value           uniform 1.2;
    }
    procBoundary2to7
    {
        type            processor;
        value           uniform 1.2;
    }
    procBoundary2to8
    {
        type            processor;
        value           uniform 1.2;
    }
    procBoundary2to10
    {
        type            processor;
        value           uniform 1.2;
    }
}


