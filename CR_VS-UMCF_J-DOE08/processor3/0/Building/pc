FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/Building";
    object      pc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform -10000000;

boundaryField
{
    sides
    {
        type            zeroGradient;
    }
    buildings_to_air
    {
        type            compressible::CFDHAMsolidMoistureCoupledImpermeable;
        value           uniform -10000000;
    }
    interior
    {
        type            zeroGradient;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform -10000000;
    }
    procBoundary3to4
    {
        type            processor;
        value           uniform -10000000;
    }
    procBoundary3to5
    {
        type            processor;
        value           uniform -10000000;
    }
}


