        setAverage      0;
        average         0;
        interpolationScheme cell;
        value           nonuniform List<scalar> 0();
    }
    bottom
    {
        type            mapped;
        field           T;
        setAverage      0;
        average         0;
        interpolationScheme cell;
        value           uniform 297;
    }
    top
    {
        type            mapped;
        field           T;
        setAverage      0;
        average         0;
        interpolationScheme cell;
        value           nonuniform List<scalar> 0();
    }
    tower
    {
        type            mapped;
        field           T;
        setAverage      0;
        average         0;
        interpolationScheme cell;
        value           uniform 297;
    }
    air_to_vegetation
    {
        type            mappedLeafTemperature;
