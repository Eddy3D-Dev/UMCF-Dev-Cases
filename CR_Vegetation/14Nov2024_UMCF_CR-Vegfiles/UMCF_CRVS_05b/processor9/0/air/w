FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      w;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0076;

boundaryField
{
    left
    {
        type            slip;
    }
    right
    {
        type            slip;
    }
    back
    {
        type            inletOutlet;
        inletValue      uniform 0.0076;
        value           uniform 0.0076;
    }
    front
    {
        type            uniformFixedValue;
        uniformValue    tableFile;
        uniformValueCoeffs
        {
            file            "$FOAM_CASE/0/air/wambient";
            format          foam;
        }
        value           nonuniform List<scalar> 0();
    }
    bottom
    {
        type            zeroGradient;
    }
    top
    {
        type            slip;
    }
    tower
    {
        type            compressible::CFDHAMfluidMoistureCoupledImpermeable;
        value           nonuniform List<scalar> 0();
    }
    procBoundary9to0
    {
        type            processor;
        value           uniform 0.0076;
    }
    procBoundary9to5
    {
        type            processor;
        value           uniform 0.0076;
    }
    procBoundary9to6
    {
        type            processor;
        value           uniform 0.0076;
    }
    procBoundary9to8
    {
        type            processor;
        value           uniform 0.0076;
    }
    procBoundary9to15
    {
        type            processor;
        value           uniform 0.0076;
    }
    procBoundary9to18
    {
        type            processor;
        value           uniform 0.0076;
    }
    procBoundary9to19
    {
        type            processor;
        value           uniform 0.0076;
    }
}


