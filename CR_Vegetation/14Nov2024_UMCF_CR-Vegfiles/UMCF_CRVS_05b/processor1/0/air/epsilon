FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    left
    {
        type            slip;
    }
    right
    {
        type            slip;
    }
    back
    {
        type            zeroGradient;
    }
    front
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    10;
        value           uniform 0.001;
    }
    bottom
    {
        type            epsilonWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0.1;
    }
    top
    {
        type            slip;
    }
    tower
    {
        type            epsilonWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0.1;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary1to4
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary1to5
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary1to7
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary1to8
    {
        type            processor;
        value           uniform 0.1;
    }
}


