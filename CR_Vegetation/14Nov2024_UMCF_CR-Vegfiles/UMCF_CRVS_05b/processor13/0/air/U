FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/air";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (10 0 0);

boundaryField
{
    left
    {
        type            slip;
    }
    right
    {
        type            slip;
    }
    back
    {
        type            inletOutlet;
        inletValue      nonuniform List<vector> 0();
        value           nonuniform List<vector> 0();
    }
    front
    {
        type            atmBoundaryLayerInletVelocity;
        Uref            10;
        Zref            10;
        zDir            ( 0 0 1 );
        flowDir         ( 1 0 0 );
        z0              uniform 1;
        zGround         uniform 0;
        value           nonuniform List<vector> 0();
    }
    bottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    top
    {
        type            slip;
    }
    tower
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    procBoundary13to12
    {
        type            processor;
        value           uniform (10 0 0);
    }
    procBoundary13to14
    {
        type            processor;
        value           uniform (10 0 0);
    }
}


