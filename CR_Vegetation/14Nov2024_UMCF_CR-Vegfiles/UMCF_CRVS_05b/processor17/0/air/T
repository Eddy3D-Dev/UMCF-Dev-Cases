FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 293.15;

boundaryField
{
    left
    {
        type            slip;
    }
    right
    {
        type            slip;
    }
    back
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    front
    {
        type            uniformFixedValue;
        uniformValue    tableFile;
        uniformValueCoeffs
        {
            file            "$FOAM_CASE/0/air/Tambient";
            format          foam;
        }
        value           uniform 299.25;
    }
    bottom
    {
        type            zeroGradient;
    }
    top
    {
        type            slip;
    }
    tower
    {
        type            compressible::CFDHAMfluidTemperatureCoupledMixed;
        value           uniform 298.15;
    }
    procBoundary17to10
    {
        type            processor;
        value           uniform 293.15;
    }
    procBoundary17to11
    {
        type            processor;
        value           uniform 293.15;
    }
    procBoundary17to12
    {
        type            processor;
        value           uniform 293.15;
    }
    procBoundary17to16
    {
        type            processor;
        value           uniform 293.15;
    }
    procBoundary17to18
    {
        type            processor;
        value           uniform 293.15;
    }
    procBoundary17to19
    {
        type            processor;
        value           uniform 293.15;
    }
}


