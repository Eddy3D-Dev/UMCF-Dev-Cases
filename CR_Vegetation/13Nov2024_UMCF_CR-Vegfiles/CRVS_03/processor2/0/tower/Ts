FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/tower";
    object      Ts;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    buildings_to_air
    {
        type            compressible::CFDHAMsolidTemperatureCoupledMixed;
        qrNbr           qr;
        qsNbr           qs;
        value           uniform 300;
    }
    interior
    {
        type            compressible::CFDHAMsolidTemperatureTransferCoeffInterior;
        hcoeff          0.405;
        Tamb            "$FOAM_CASE/0/tower/T_room.dat";
        value           uniform 300;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary2to4
    {
        type            processor;
        value           uniform 300;
    }
}


