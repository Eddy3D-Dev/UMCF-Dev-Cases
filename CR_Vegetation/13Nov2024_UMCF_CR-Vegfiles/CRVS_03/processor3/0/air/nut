FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    left
    {
        type            slip;
    }
    right
    {
        type            slip;
    }
    back
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    front
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    bottom
    {
        type            nutkAtmRoughWallFunction;
        z0              uniform 1;
        value           uniform 0.1;
    }
    top
    {
        type            slip;
    }
    tower
    {
        type            nutkWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0.1;
    }
    procBoundary3to0
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary3to4
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary3to7
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary3to11
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary3to13
    {
        type            processor;
        value           uniform 0.1;
    }
}


