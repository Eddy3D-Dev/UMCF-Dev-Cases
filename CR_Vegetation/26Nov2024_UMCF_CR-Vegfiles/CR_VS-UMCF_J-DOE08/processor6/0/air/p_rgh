FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    left
    {
        type            slip;
    }
    right
    {
        type            slip;
    }
    back
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    front
    {
        type            fixedFluxPressure;
        gradient        nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    bottom
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 100000;
    }
    top
    {
        type            slip;
    }
    Building
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 100000;
    }
    procBoundary6to1
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary6to2
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary6to3
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary6to7
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary6to8
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary6to9
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary6to11
    {
        type            processor;
        value           uniform 100000;
    }
}


